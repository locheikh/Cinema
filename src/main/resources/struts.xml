	<constant name="struts.custom.i18n.resources" value="label" />

	<package name="example" extends="struts-default">
		
        <action name="HelloWorld" class="example.HelloWorld">
			<result name="success">/pages/HelloWorld.jsp</result>
		</action>

		
	  <action name="MovieAction" class="Action.MovieAction">
			<result name="success">/pages/Movie.jsp</result>
			<result name="error">/pages/error.jsp</result>
	  </action>
	  
	   <action name="MovieTheatreAction" class="Action.MovieTheatreAction">
			<result name="success">/pages/MovieTheatres.jsp</result>
			<!-- <result name="success" type="redirectAction">MovieAction?id=${id}</result> -->
			<result name="error">/pages/error.jsp</result>
	  </action>
	  
	<action name="SessionAction" class="Action.SessionAction">
			<result name="success">/pages/Session.jsp</result>
			<result name="error">/pages/error.jsp</result>
	  </action> 
	  
	
	  
